<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nomad_mystic_lab1.lab1_activity" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DarkActivity"
            android:label="@string/title_activity_dark"
            android:parentActivityName=".MainActivity"
            android:theme="@style/dark_theme" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.nomad_mystic_lab1.lab1_activity.MainActivity" />
        </activity>
        <activity
            android:name=".light_Dark_bar"
            android:label="@string/title_activity_light__dark_bar"
            android:parentActivityName=".MainActivity"
            android:theme="@style/light_dark_bar_theme" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.nomad_mystic_lab1.lab1_activity.MainActivity" />
        </activity>
        <activity
            android:name=".CustomActivity"
            android:label="@string/title_activity_custom"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.nomad_mystic_lab1.lab1_activity.MainActivity" />
        </activity>
    </application>

</manifest>
